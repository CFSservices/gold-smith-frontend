# Gold Smith Frontend - UAT Environment
# Used for: User Acceptance Testing

# API Configuration
VITE_API_BASE_URL=https://uat-api.goldsmith.example.com/api/v1
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Gold Smith (UAT)
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=uat

# Feature Flags
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_DEVTOOLS=false

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=gs_access_token
VITE_AUTH_REFRESH_KEY=gs_refresh_token
VITE_AUTH_TOKEN_EXPIRY=3600
