# Gold Smith Frontend - Development Environment
# Used for: Development server deployment

# API Configuration
VITE_API_BASE_URL=https://dev-api.goldsmith.example.com/api/v1
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Gold Smith (Dev)
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_MOCK_API=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_DEVTOOLS=true

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=gs_access_token
VITE_AUTH_REFRESH_KEY=gs_refresh_token
VITE_AUTH_TOKEN_EXPIRY=3600
