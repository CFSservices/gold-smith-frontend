# Gold Smith Frontend - QA Environment
# Used for: Quality Assurance testing

# API Configuration
VITE_API_BASE_URL=https://qa-api.goldsmith.example.com/api/v1
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Gold Smith (QA)
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=qa

# Feature Flags
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_DEVTOOLS=true

# Authentication Configuration
VITE_AUTH_TOKEN_KEY=gs_access_token
VITE_AUTH_REFRESH_KEY=gs_refresh_token
VITE_AUTH_TOKEN_EXPIRY=3600
